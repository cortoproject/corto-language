bool result : true

void fail(string msg):
    "tc_function06c: FAIL: $msg"
    result = false

struct Point:: x, y: int32

void func_nested(Point p, Point q, int32 px, int32 py, int32 qx, int32 qy):
    if p.x != px: fail("p.x != $px")
    if p.y != py: fail("p.y != $py")
    if "$p" != "{$px,$py}": fail("\"$p\" != \"{$px,$py}\"")
    if q.x != qx: fail("q.x != qx")
    if q.y != qy: fail("q.y != qy")
    if "$q" != "{$qx,$qy}": fail("\"$q\" != \"{$qx,$qy}\"")

void func(Point p, Point q):
    if p.x != 10: fail("p.x != 10")
    if p.y != 20: fail("p.y != 20")
    if "$p" != "{10,20}": fail("\"$p\" != \"{10,20}\"")
    if q.x != 30: fail("q.x != 30")
    if q.y != 40: fail("q.y != 40")
    if "$q" != "{30,40}": fail("\"$q\" != \"{30,40}\"")
    func_nested(p, q, 10, 20, 30, 40)

