bool result: true
void fail(string msg):
    "tc_resolve01: FAIL: $msg"
    result = false

// Test which class is resolved first
void class
var object t = @::.resolve("class")
if t != ::corto::lang::class: fail("t != ::corto::lang::class #1")

t = @::.resolve("lang::class")
if t != ::corto::lang::class: fail("t != ::corto::lang::class #2")

t = @::.resolve("corto::lang::class")
if t != ::corto::lang::class: fail("t != ::corto::lang::class #3")

t = @::.resolve("::corto::lang::class")
if t != ::corto::lang::class: fail("t != ::corto::lang::class #4")

t = @::.resolve("::class")
if t != ::class: fail("t != ::class")

if result: "tc_resolve01: OK"