if l.line2.start.y != 7: fail("l.line2.start.y != 7")
if l.line2.stop.x != 8: fail("l.line2.stop.x != 8")
if l.line2.stop.y != 9: fail("l.line2.stop.y != 9")
if "$l" != "{<1>::Line{<2>::Point{2,3},<3>::Point{4,5}},<4>::Line{<5>::Point{6,7},<6>::Point{8,9}}}": fail("\"l\" != \"{<1>::Line{<2>::Point{2,3},<3>::Point{4,5}},<4>::Line{<5>::Point{6,7},<6>::Point{8,9}}}\"")

if result: "tc_storage13b: OK"
